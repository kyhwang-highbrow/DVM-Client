<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tnkfactory.offerwall"
    android:versionCode="197"
    android:versionName="1.97" >
 
    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	
    <application
        android:icon="@drawable/tnk_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
            <!-- deeplink test scheme -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:host="open" android:scheme="tnk-ad"/>
            </intent-filter>
        </activity>
        <activity android:name=".OfferwallEmbedActivity" android:theme="@android:style/Theme.NoTitleBar"
            />
        
        <activity android:name="com.tnkfactory.ad.AdWallActivity" />
        <activity android:name="com.tnkfactory.ad.AdMediaActivity" android:screenOrientation="landscape"/>
        
        <receiver android:name="com.tnkfactory.ad.TnkReceiver" android:exported="true"> 
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" /> 
            </intent-filter>
        </receiver>
        
        <meta-data android:name="tnkad_app_id" android:value="30408070-4051-9322-2239-15040708030f" />
    </application>


</manifest>
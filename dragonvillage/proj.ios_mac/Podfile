  # Pods for Google
  pod 'GoogleSignIn'

  # Pods for Facebook
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'

  # Pods for IGAWorks
  pod 'IgaworksCore'
  pod 'AdBrix'

  # Pods for Tapjoy
  pod 'TapjoySDK'

  # Pods for AdColony
  pod 'AdColony'

  # Pods for Adjust
  pod 'Adjust'

end

